{
    "author": "42!",
    "background": {
        "persistent": false,
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/logo.png"
    },
    "commands": {
        "trigger_fill_all_forms": {
            "description": "Lorem Fill this page",
            "suggested_key": {
                "default": "Ctrl+Shift+1",
                "mac": "Command+Shift+1"
            }
        }
    },
    "content_scripts": [
        {
            "js": [
                "js/libs/jquery-1.11.3.min.js",
                "js/libs/underscore-min.js",
                "js/libs/string.js",
                "js/libs/chance.min.js",
                "js/listeners.js",
                "js/matchers.js",
                "js/value-decider.js",
                "js/strategies.js",
                "js/filler.js"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ],
    "description": "Fill context specific random test data into forms",
    "icons": {
        "128": "icons/logo.png",
        "16": "icons/logo.png",
        "48": "icons/logo.png"
    },
    "manifest_version": 2,
    "name": "Lorem Fill",
    "offline_enabled": true,
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "short_name": "LoremFill",
    "version": "0.3"
}
